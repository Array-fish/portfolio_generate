    "start-static-gp": "http-server -c-1 ./docs",
    "preview": "nuxi preview"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "nuxt": "3.0.0-rc.6",
    "tailwindcss": "^3.1.6"
  },
  "dependencies": {
    "daisyui": "^2.19.1",
    "nuxi": "^3.0.0-rc.6"
  }
}
